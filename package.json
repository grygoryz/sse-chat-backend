    "class-validator": "^0.13.1",
    "connect-redis": "^6.0.0",
    "dotenv": "^10.0.0",
    "express-session": "^1.17.2",
    "nestjs-pino": "^2.2.0",
    "pg": "^8.7.1",
    "pino-http": "^5.8.0",
    "pino-pretty": "^7.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.37",
    "typeorm-seeding": "^1.6.1"
  },
  "devDependencies": {
